<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="de.umg.mi.idrt.idrtimporttool.importidrt.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="de.umg.mi.idrt.importtool.application"
            name="IDRT Import Tool">
         <property
               name="appName"
               value="IDRT Import Tool">
         </property>
        <property
              name="aboutText"
              value="The IDRT Import Tool is part of the Integrated Data Repository Toolkit (http://www.tmf-ev.de/Themen/Projekte/V091_MI_IDRT.aspx) which was funded by TMF – Infrastructures for Medical Research (http://www.tmf-ev.de).&#x0A;Data integration jobs created using &lt;Talend Open Studio for Data Integration&gt; with the job files being freely available under GNU LGPL (http://www.gnu.org/licenses/lgpl.html) at http://idrt.imise.uni-leipzig.de. GUI and platform created using &lt;Eclipse Rich Client Platform&gt;.&#x0A;&#x0A;License condition and copyright: &#x0A;This software, including all of its parts and accompanying documents, is protected by copyright. The rights belong to TMF unless otherwise indicated. TMF is unable to give a guarantee with regard to the accuracy of content and error-free operation. Source code is made available by TMF for the purpose of customizing and improving the software. Inasmuch as software based on changed source code is used in practice, the changed source code shall be made available to TMF in digitized form free of charge, without any exploitation restrictions, for the purpose of further developing and improving the software, to the extent not precluded by legal regulations or contractual obligations (including those toward third parties). Reproduction and transmission, also of changed source code, are permitted. Any references to TMF&apos;s copyright shall be left in the code and the text file with the references to TMF&apos;s conditions of use shall be enclosed.&#x0A;&#x0A;Contact:&#x0A;IDRT Import Tool and CSV-, SQL-, ODM-Import-Modules: benjamin.baum@med.uni-goettingen.de; christian.bauer@med.uni-goettingen.de&#x0A;§21-Import-Module: igor.engel@imi.med.uni-erlangen.de; thomas.ganslandt@uk-erlangen.de&#x0A;Standard terminologies-Import-Module: matthias.loebe@imise.uni-leipzig.de; sebastian.staeubert@imise.uni-leipzig.de"/> 
         <property
               name="windowImages"
               value="images/tmf.jpg">
         </property>
         <property
               name="aboutImage"
               value="images/tmf.jpg">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="de.umg.mi.idrt.importtool.IDRT"
            name="IDRT">
      </category>
      <view
            allowMultiple="false"
            category="de.umg.mi.idrt.importtool.IDRT"
            class="de.umg.mi.idrt.importtool.views.ServerView"
            icon="images/addServer.png"
            id="de.umg.mi.idrt.importtool.ServerView"
            name="IDRT Import Tool">
      </view>
   </extension>
    <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.commands.ODMImportCommand"
            id="de.goettingen.i2b2.importtool.ODMImport"
            name="ODMImport">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.commands.CSVImportCommand"
            id="de.goettingen.i2b2.importtool.CSVImport"
            name="CSVImport">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.commands.DBImportCommand"
            id="de.goettingen.i2b2.importtool.DBImport"
            name="DBImport">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.commands.P21ImportCommand"
            id="de.goettingen.i2b2.importtool.P21Import"
            name="P21Import">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.server.commands.DeleteTargetServerCommand"
            id="de.goettingen.i2b2.importtool.idrt.deleteServer"
            name="DeleteTargetServer">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.server.commands.EditTargetServerCommand"
            id="de.goettingen.i2b2.importtool.idrt.editServer"
            name="EditTargetServer">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.server.commands.AddTargetServerCommand"
            id="de.goettingen.i2b2.importtool.idrt.addServer"
            name="AddTargetServer">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.server.commands.DeleteAllTargetServersCommand"
            id="de.goettingen.i2b2.importtool.idrt.deleteAllServers"
            name="DeleteAllTargetServers">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.server.commands.AdminTargetServerCommand"
            id="de.goettingen.i2b2.importtool.idrt.adminServer"
            name="AdminTargetServer">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.server.commands.ExportTargetServerCommand"
            id="de.goettingen.i2b2.importtool.idrt.exportServer"
            name="ExportTargetServer">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.server.commands.ImportTargetServerCommand"
            id="de.goettingen.i2b2.importtool.idrt.importServer"
            name="ImportTargetServer">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.server.commands.RefreshCommand"
            id="de.goettingen.i2b2.importtool.idrt.refresh"
            name="Refresh">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.server.commands.AddSourceServerCommand"
            id="de.goettingen.i2b2.importtool.idrt.addSourceServer"
            name="AddSourceServer">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.server.commands.DeleteSourceServerCommand"
            id="de.goettingen.i2b2.importtool.idrt.deleteSourceServer"
            name="DeleteSourceServer">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.server.commands.EditSourceServerCommand"
            id="de.goettingen.i2b2.importtool.idrt.editSourceServer"
            name="EditSourceServer">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.server.commands.ExportSourceServerCommand"
            id="de.goettingen.i2b2.importtool.idrt.exportSourceServer"
            name="ExportSourceServer">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.server.commands.ImportSourceServerCommand"
            id="de.goettingen.i2b2.importtool.idrt.importSourceServer"
            name="ImportSourceServer">
      </command>
      <command
            defaultHandler="org.eclipselabs.p2.rcpupdate.utils.handler.InstallNewSoftwareHandler"
            id="org.eclipselabs.p2.rcpupdate.install"
            name="Install New Software">
      </command>
      <command
            defaultHandler="org.eclipselabs.p2.rcpupdate.utils.handler.UpdateHandler"
            id="org.eclipselabs.p2.rcpupdate.update"
            name="Update Software">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.commands.ReportBugCommand"
            id="de.umg.mi.idrt.importtool.reportBug"
            name="reportBug">
      </command>
      <command
            defaultHandler="de.umg.mi.idrt.idrtimporttool.commands.DocumentationCommand"
            id="de.umg.mi.idrt.importtool.documentation"
            name="Documentation">
      </command>
   </extension>
