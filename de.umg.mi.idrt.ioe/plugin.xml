<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application
            icon="images/tmf.png"
            visible="true">
         <run
               class="de.umg.mi.idrt.ioe.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="de.umg.mi.idrt.ioe.PerspectiveSimple"
            id="de.umg.mi.idrt.ioe.PerspectiveSimple"
            name="PerspectiveSimple">
      </perspective>
      <perspective
            class="de.umg.mi.idrt.ioe.PerspectiveExpert"
            id="de.umg.mi.idrt.ioe.PerspectiveExpert"
            name="PerspectiveExpert">
      </perspective>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="de.umg.mi.idrt.ioe.application"
            name="IDRT Import and Mapping Tool">
         <property
               name="appName"
               value="IDRT Import and Mapping Tool">
         </property>
         <property
               name="aboutImage"
               value="images/tmf.jpg">
         </property>
         <property
               name="aboutText"
               value="The IDRT Import and Mapping Tool (IMT) is part of the Integrated Data Repository Toolkit (http://www.tmf-ev.de/Themen/Projekte/V091_MI_IDRT.aspx) which was funded by TMF – Infrastructures for Medical Research (http://www.tmf-ev.de).&#x0D;&#x0A;Data integration jobs created using &lt;Talend Open Studio for Data Integration&gt; with the job files being freely available under GNU LGPL (http://www.gnu.org/licenses/lgpl.html) at http://idrt.imise.uni-leipzig.de. GUI and platform created using &lt;Eclipse Rich Client Platform&gt;.&#x0D;&#x0A;&#x0D;&#x0A;This IMT is released under GNU GPLv2 (http://www.gnu.org/licenses)  with all ETL created with Talend Open Studio licensed as GNU LGPL. For details see &lt;LICENSE.txt&gt;.&#x0D;&#x0A;&#x0D;&#x0A;Contact:&#x0D;&#x0A;IDRT Import and Mapping Tool and CSV-, SQL-, ODM-Import-Modules: benjamin.baum@med.uni-goettingen.de; christian.bauer@med.uni-goettingen.de&#x0D;&#x0A;§21-Import-Module: igor.engel@imi.med.uni-erlangen.de; thomas.ganslandt@uk-erlangen.de&#x0D;&#x0A;Standard terminologies-Import-Module: matthias.loebe@imise.uni-leipzig.de; sebastian.staeubert@imise.uni-leipzig.de">
         </property>
         <property
               name="windowImages"
               value="icons/i2b2_hive_16.gif,icons/i2b2_hive_32.gif,icons/i2b2_hive_48.gif,icons/i2b2_hive_64.gif,icons/i2b2_hive_128.gif">
         </property>
      </product>
   </extension>
   
   
   <extension
         point="org.eclipse.ui.views">
            <category
            id="de.umg.mi.idrt.importtool.IDRT"
            name="IDRT">
      </category>
      <view
            allowMultiple="false"
            category="de.umg.mi.idrt.importtool.IDRT"
            class="de.umg.mi.idrt.importtool.views.ServerView"
            icon="images/addServer.png"
            id="de.umg.mi.idrt.importtool.ServerView"
            name="Import Browser">
      </view>
      <view
            allowMultiple="true"
            category="de.umg.mi.idrt.importtool.IDRT"
            class="de.umg.mi.idrt.ioe.view.StatusView"
            icon="images/simpletype_obj.gif"
            id="edu.goettingen.i2b2.importtool.view.StatusView"
            name="StatusView"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="de.umg.mi.idrt.importtool.IDRT"
            class="de.umg.mi.idrt.ioe.view.EditorStagingInfoView"
            icon="images/go-next.png"
            id="de.umg.mi.idrt.ioe.EditorStagingInfoView"
            name="Staging Info View"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="de.umg.mi.idrt.importtool.IDRT"
            class="de.umg.mi.idrt.ioe.view.EditorTargetInfoView"
            icon="images/go-next-unchecked.png"
            id="de.umg.mi.idrt.ioe.EditorTargetInfoView"
            name="Target Info View"
            restorable="true">
      </view>
      <view
            category="de.umg.mi.idrt.importtool.IDRT"
            class="de.umg.mi.idrt.ioe.view.OntologyEditorView"
            icon="images/format-justify-fill.png"
            id="de.umg.mi.idrt.ioe.OntologyEditor"
            name="Mapping Editor"
            restorable="true">
      </view>
      <view
            category="de.umg.mi.idrt.importtool.IDRT"
            class="de.umg.mi.idrt.ioe.view.ProgressView"
            icon="images/progress_none.gif"
            id="de.umg.mi.idrt.ioe.ProgressView"
            name="Mapping Editor Progress"
            restorable="true">
      </view>
      <view
            class="de.umg.mi.idrt.ioe.view.HelpView"
            id="de.umg.mi.idrt.ioe.help"
            name="Help"
            restorable="true">
      </view>
   </extension>
