            </command>
            <separator
                  name="de.umg.mi.idrt.ioe.separator2"
                  visible="true">
            </separator>
         </menu>
         <menu
               label="Target i2b2">
            <command
                  commandId="de.umg.mi.idrt.ioe.newInstance"
                  label="New Instance..."
                  style="push">
            </command>
         </menu>
         <menu
               label="Options">
            <command
                  commandId="de.umg.mi.idrt.ioe.options"
                  label="Options..."
                  style="push">
            </command>
            <command
                  commandId="de.umg.mi.idrt.ioe.addRegex"
                  label="Add Regular Expression..."
                  style="push">
            </command>
         </menu>
