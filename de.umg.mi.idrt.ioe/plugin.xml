             id="de.umg.mi.idrt.ioe.addRegex"
             name="addRegex">
       </command>
       <command
             defaultHandler="de.umg.mi.idrt.ioe.commands.OntologyEditor.OptionsCommand"
             id="de.umg.mi.idrt.ioe.options"
             name="options">
       </command>
