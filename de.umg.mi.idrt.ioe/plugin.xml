      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="file.id"
               label="File">
            <command
                  commandId="org.eclipse.ui.file.exit"
                  label="Exit">
            </command>
         </menu>
         <menu
               label="Server">
            <command
                  commandId="de.goettingen.i2b2.importtool.idrt.addServer"
                  label="Add Staging I2B2Server..."
